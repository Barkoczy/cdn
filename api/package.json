{
    "name": "cdn-management-api",
    "version": "1.0.0",
    "description": "API pre správu CDN súborov postavené na Hono",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc && tsc-alias",
        "start": "node dist/index.js",
        "test": "vitest run",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "lint:watch": "eslint src --watch",
        "lint:fix:all": "eslint . --fix",
        "db:migrate": "prisma migrate dev",
        "db:deploy": "prisma migrate deploy",
        "db:generate": "prisma generate"
    },
    "keywords": [
        "cdn",
        "api",
        "hono",
        "typescript"
    ],
    "dependencies": {
        "@hono/node-server": "^1.2.0",
        "@hono/zod-validator": "^0.4.3",
        "@prisma/client": "^6.4.1",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "bull": "^4.12.0",
        "crypto": "^1.0.1",
        "hono": "^4.7.0",
        "ioredis": "^5.3.2",
        "jose": "^5.1.3",
        "mime-types": "^2.1.35",
        "multer": "^1.4.5-lts.1",
        "nanoid": "^5.0.4",
        "prom-client": "^15.1.3",
        "sharp": "^0.33.0",
        "stream-throttle": "^0.1.3",
        "tsc-alias": "^1.8.11",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@types/bcrypt": "^5.0.2",
        "@types/mime-types": "^2.1.4",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.5",
        "@types/stream-throttle": "^0.1.4",
        "@types/throttle": "^1.0.4",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.57.1",
        "globals": "^16.0.0",
        "prisma": "^6.4.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.26.0",
        "vitest": "^3.0.7"
    }
}
